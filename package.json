  "author": "Ziphil",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "develop": "run-p develop:client develop:server develop:nodemon",
    "develop:client": "webpack-dev-server --mode development --devtool inline-source-map --config webpack-client.ts --hot",
    "develop:server": "webpack --mode development --config webpack-server.ts --watch",
    "develop:nodemon": "nodemon --inspect -L dist/index.js",
    "develop:service": "node dist/service.js",
    "build": "run-s build:client build:server",
    "build:client": "webpack -p --mode production --config webpack-client.ts",
    "build:client:analyze": "cross-env ANALYZE=1 npm run build:client",
    "build:server": "webpack --mode production --config webpack-server.ts",
    "start": "node dist/index.js",
    "start:service": "node dist/service.js",
    "postinstall": "npm run build",
    "lint": "eslint client/**/*.ts client/**/*.tsx server/**/*.ts",
    "command:tsnode": "ts-node -r tsconfig-paths/register",
    "command:tsc": "tsc"
  },
  "eslintConfig": {
    "extends": "ziphil",
    "ignorePatterns": "webpack-*.ts"
  },
  "nodemonConfig": {
    "watch": [
      "dist"
    ],
    "ext": "js",
    "ignore": [
      "dist/script/**",
      "dist/static/**",
      "dist/client/**",
      "dist/main*"
    ]
